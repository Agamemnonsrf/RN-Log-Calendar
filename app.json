{
  "expo": {
    "name": "Log Calendar",
    "slug": "RN-Log-Calendar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-new.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#2B303A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-new.png",
        "backgroundColor": "#2B303A"
      },
      "package": "com.RNLogCalendar.RNLogCalendar"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6138a021-c6c9-4653-a2c3-7c716ce63dc1"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
